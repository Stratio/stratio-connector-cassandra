<?xml version="1.0" encoding="UTF-8"?>
<DataStore>
    <Name>Cassandra</Name>
    <Version>2.0.0</Version>
    <RequiredProperties>
        <cluster>
            <name>CassandraCluster</name>
            <node>
                <host>127.0.0.1</host>
                <port>9042</port>
            </node>
        </cluster>
    </RequiredProperties>
    <OptionalProperties>
    </OptionalProperties>
    <Behaviors>
        <Behavior>UPSERT_ON_INSERT</Behavior>
    </Behaviors>
</DataStore>



